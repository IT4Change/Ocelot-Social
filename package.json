    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "sirv": "^2.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vike": "^0.4.145",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vuetify": "^3.4.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
